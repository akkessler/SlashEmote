{
  "metadata": {
    "schemaVersion": "1.0",
    "importType": "LEX",
    "importFormat": "JSON"
  },
  "resource": {
    "name": "SlashEmote",
    "version": "1",
    "intents": [
      {
        "name": "EmoteIntent",
        "version": "1",
        "fulfillmentActivity": {
          "type": "ReturnIntent"
        },
        "sampleUtterances": [
          "slash {command}",
          "{command}"
        ],
        "slots": [
          {
            "name": "command",
            "slotConstraint": "Required",
            "slotType": "Emotes",
            "slotTypeVersion": "1",
            "valueElicitationPrompt": {
              "messages": [
                {
                  "contentType": "PlainText",
                  "content": "What emote?"
                }
              ],
              "maxAttempts": 2
            },
            "priority": 1,
            "sampleUtterances": []
          }
        ]
      }
    ],
    "slotTypes": [
      {
        "description": "Available WoW emotes",
        "name": "Emotes",
        "version": "1",
        "enumerationValues": [
          {
            "value": "hello",
            "synonyms": []
          }
        ],
        "valueSelectionStrategy": "TOP_RESOLUTION"
      }
    ],
    "voiceId": "0",
    "childDirected": false,
    "locale": "en-US",
    "idleSessionTTLInSeconds": 60,
    "clarificationPrompt": {
      "messages": [
        {
          "contentType": "PlainText",
          "content": "Sorry, can you please repeat that?"
        }
      ],
      "maxAttempts": 5
    },
    "abortStatement": {
      "messages": [
        {
          "contentType": "PlainText",
          "content": "Sorry, I could not understand. Goodbye."
        }
      ]
    }
  }
}